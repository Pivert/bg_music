#: init.lua
msgid "Music is currently disabled for you. Use @1 to enable it."
msgstr "A música está atualmente desativada para você. Use @1 para ativá-la."

#: init.lua
msgid "Music is enabled for you. Use @1 to disable it."
msgstr "A música está ativada para você. Use @1 para desativá-la."

#: init.lua
msgid "Music disabled. You will no longer hear automatic background music."
msgstr "Música desativada. Você não ouvirá mais a música de fundo automática."

#: init.lua
msgid "Music enabled. You will now hear background music."
msgstr "Música ativada. Agora você ouvirá a música de fundo."

#: init.lua
msgid "Failed to disable music."
msgstr "Falha ao desativar a música."

#: init.lua
msgid "Failed to enable music."
msgstr "Falha ao ativar a música."

#: init.lua
msgid "Set your personal music volume (0-100%)"
msgstr "Defina seu volume de música pessoal (0-100%)"

#: init.lua
msgid "Usage: @1 <volume> (0-100)"
msgstr "Uso: @1 <volume> (0-100)"

#: init.lua
msgid "Volume set to @1%"
msgstr "Volume definido para @1%"

#: init.lua
msgid "Volume must be between 0 and 100"
msgstr "O volume deve estar entre 0 e 100"

#: init.lua
msgid "Music is disabled for you. Use @1 to enable it."
msgstr "A música está desativada para você. Use @1 para ativá-la."

#: init.lua
msgid "Stereo music stopped"
msgstr "Música estéreo parada"

#: init.lua
msgid "No music files found in sounds/ directory"
msgstr "Nenhum arquivo de música encontrado no diretório sounds/"

#: init.lua
msgid "Playing: @1"
msgstr "Tocando: @1"

#: init.lua
msgid "Play a song by number (from /listmusic) or name. Use 'all' to play globally"
msgstr "Tocar uma música por número (de /listmusic) ou nome. Use 'all' para tocar globalmente"

#: init.lua
msgid "No players available to hear the music (all have disabled music)"
msgstr "Nenhum jogador disponível para ouvir a música (todos desativaram a música)"

#: init.lua
msgid "You have disabled music. Use @1 first."
msgstr "Você desativou a música. Use @1 primeiro."

#: init.lua
msgid "Invalid song number. Use 1-@1"
msgstr "Número de música inválido. Use 1-@1"

#: init.lua
msgid "No music available"
msgstr "Nenhuma música disponível"

#: init.lua
msgid "Song not found: @1. Use @2 to see available songs"
msgstr "Música não encontrada: @1. Use @2 para ver as músicas disponíveis"

#: init.lua
msgid "Player not found"
msgstr "Jogador não encontrado"

#: init.lua
msgid "Failed to play music"
msgstr "Falha ao tocar a música"

#: init.lua
msgid "Stop all background music currently playing"
msgstr "Parar toda a música de fundo em reprodução"

#: init.lua
msgid "Stopped @1 music instances"
msgstr "Parou @1 instâncias de música"

#: init.lua
msgid "No music currently playing"
msgstr "Nenhuma música tocando no momento"

#: init.lua
msgid "Disable all background music for yourself"
msgstr "Desative toda a música de fundo para você"

#: init.lua
msgid "Enable background music for yourself"
msgstr "Ative a música de fundo para você"

#: init.lua
msgid "Set background music at current position. radius_trigger: distance where music starts (meters). extra_radius: additional distance where music stops (meters). volume: 5-99 (default 65). filter: optional song filter (supports wildcards, | for OR, ! for NOT)."
msgstr "Definir música de fundo na posição atual. raio_gatilho: distância onde a música começa (metros). raio_extra: distância adicional onde a música para (metros). volume: 5-99 (padrão 65). filtro: filtro opcional de músicas (suporta curingas, | para OU, ! para NÃO)."

#: init.lua
msgid "Usage: @1 <radius_trigger> <extra_radius> [<volume>] [<filter>]"
msgstr "Uso: @1 <raio_gatilho> <raio_extra> [<volume>] [<filtro>]"

#: init.lua
msgid "Radius trigger must be a positive number"
msgstr "O raio de gatilho deve ser um número positivo"

#: init.lua
msgid "Extra radius must be a non-negative number"
msgstr "O raio extra deve ser um número não negativo"

#: init.lua
msgid "Volume must be between @1 and @2"
msgstr "O volume deve estar entre @1 e @2"

#: init.lua
msgid "Player not found"
msgstr "Jogador não encontrado"

#: init.lua
msgid "Background music location set at @1 with trigger radius @2 and extra radius @3"
msgstr "Localização da música de fundo definida em @1 com raio de gatilho @2 e raio extra @3"

#: init.lua
msgid "Background music location set at @1 with trigger radius @2 and extra radius @3 (filter: @4)"
msgstr "Localização da música de fundo definida em @1 com raio de gatilho @2, raio extra @3 (filtro: @4)"

#: init.lua
msgid "List all background music locations with coordinates, radius, volume, and filters"
msgstr "Listar todas as localizações de música de fundo com coordenadas, raio, volume e filtros"

#: init.lua
msgid "No background music locations defined"
msgstr "Nenhuma localização de música de fundo definida"

#: init.lua
msgid "Background music locations:"
msgstr "Localizações de música de fundo:"

#: init.lua
msgid "Invalid index. Use @1 to see valid indices"
msgstr "Índice inválido. Use @1 para ver índices válidos"

#: init.lua
msgid "Removed background music location at @1"
msgstr "Localização de música de fundo removida em @1"

#: init.lua
msgid "List available music files with optional filter (supports wildcards, | for OR, ! for NOT)"
msgstr "Listar arquivos de música disponíveis com filtro opcional (suporta curingas, | para OU, ! para NÃO)"

#: init.lua
msgid "(Rescanned: @1 → @2 songs) "
msgstr "(Reescaneado: @1 → @2 músicas) "

#: init.lua
msgid "No songs match the filter: @1"
msgstr "Nenhuma música corresponde ao filtro: @1"

#: init.lua
msgid "Available music files"
msgstr "Arquivos de música disponíveis"

#: init.lua
msgid "Available music files (filter: @1):"
msgstr "Arquivos de música disponíveis (filtro: @1):"

#: init.lua
msgid "Manually trigger a rescan of the music folder"
msgstr "Acionar manualmente uma nova varredura da pasta de música"

#: init.lua
msgid "Music folder rescanned. Found @1 songs (was @2)"
msgstr "Pasta de música reescaneada. Encontradas @1 músicas (era @2)"

#: init.lua
msgid "Music Stereo"
msgstr "Estéreo Musical"