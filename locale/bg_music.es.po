#: init.lua
msgctxt "bg_music"
msgid "Music is currently disabled for you. Use @1 to enable it."
msgstr "La música está actualmente desactivada para ti. Usa @1 para activarla."

#: init.lua
msgctxt "bg_music"
msgid "Music is enabled for you. Use @1 to disable it."
msgstr "La música está activada para ti. Usa @1 para desactivarla."

#: init.lua
msgctxt "bg_music"
msgid "Music disabled. You will no longer hear automatic background music."
msgstr "Música desactivada. Ya no escucharás la música de fondo automática."

#: init.lua
msgctxt "bg_music"
msgid "Music enabled. You will now hear background music."
msgstr "Música activada. Ahora escucharás la música de fondo."

#: init.lua
msgctxt "bg_music"
msgid "Failed to disable music."
msgstr "Error al desactivar la música."

#: init.lua
msgctxt "bg_music"
msgid "Failed to enable music."
msgstr "Error al activar la música."

#: init.lua
msgctxt "bg_music"
msgid "Set your personal music volume (0-100%)"
msgstr "Establece tu volumen musical personal (0-100%)"

#: init.lua
msgctxt "bg_music"
msgid "Usage: @1 <volume> (0-100)"
msgstr "Uso: @1 <volumen> (0-100)"

#: init.lua
msgctxt "bg_music"
msgid "Volume set to @1%"
msgstr "Volumen establecido al @1%"

#: init.lua
msgctxt "bg_music"
msgid "Volume must be between 0 and 100"
msgstr "El volumen debe estar entre 0 y 100"

#: init.lua
msgctxt "bg_music"
msgid "Music is disabled for you. Use @1 to enable it."
msgstr "La música está desactivada para ti. Usa @1 para activarla."

#: init.lua
msgctxt "bg_music"
msgid "Stereo music stopped"
msgstr "Música estéreo detenida"

